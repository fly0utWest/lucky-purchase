generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  login     String   @unique
  name      String
  password  String
  createdAt DateTime @default(now())
}

model Item {
  id     String   @id @default(uuid())
  title  String
  images String[]
}
